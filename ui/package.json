{
  "name": "ui",
  "version": "0.0.0",
  "scripts": {
    "start": "vite --clearScreen false",
    "build": "vite build",
    "lint": "eslint src && prettier \"**/*.ts\" --check --ignore-path .gitignore",
    "format": "eslint src && prettier \"**/*.ts\" --write --ignore-path .gitignore",
